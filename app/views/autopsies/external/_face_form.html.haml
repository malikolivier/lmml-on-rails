= form_for(exam, builder: VueFormBuilder, html: { id: 'external_face_examination' }, url: '') do |f|
  = f.hidden_field :id
  .row
    .col-xs-6
      = f.select :congestion, translated_congestions, prompt: true
    .col-xs-6
      = f.select :swelling, translated_swellings, prompt: true
  = f.text_field :nasal_cavity_content
  = yield f
