<p id="notice"><%= notice %></p>

<h1>In Body Orientations</h1>

<table>
  <thead>
    <tr>
      <th>Coordinate system</th>
      <th>X</th>
      <th>Y</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @in_body_orientations.each do |in_body_orientation| %>
      <tr>
        <td><%= in_body_orientation.coordinate_system %></td>
        <td><%= in_body_orientation.x %></td>
        <td><%= in_body_orientation.y %></td>
        <td><%= link_to 'Show', in_body_orientation %></td>
        <td><%= link_to 'Edit', edit_in_body_orientation_path(in_body_orientation) %></td>
        <td><%= link_to 'Destroy', in_body_orientation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New In Body Orientation', new_in_body_orientation_path %>
